{
  "name": "Kabyle Gamers Premium",
  "version": "0.5.0",
  "description": "Ne ratez plus aucun événement de Kabyle Gamers !",
    "manifest_version": 2,
    "icons": {
      "16": "static/icon-def-16.png",
      "32": "static/icon-def-32.png",
      "48": "static/icon-def-48.png",
      "128": "static/icon-def-128.png"
    },
    "browser_action":{
      "default_popup":"popup.html"
    },
  "background": {
    "persistent": false,
    "scripts": [
      "OneSignal.js",
      "background.js"
    ]
  },
  "permissions": [
    "gcm",
    "storage",
    "notifications",
    "identity",
    "tabs"
  ]
}
